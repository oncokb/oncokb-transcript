{
  "name": "oncokb-curation",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "description": "Description for oncokb-curation",
  "license": "UNLICENSED",
  "scripts": {
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "yarn run lint -- --fix",
    "cleanup": "rimraf target/classes/static/",
    "clean-www": "rimraf target/classes/static/app/{src,target/}",
    "updateAPI": "openapi-generator-cli generate -i http://localhost:9090/v3/api-docs -g typescript-axios -o src/main/webapp/app/shared/api/generated",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "jest:update": "yarn run jest -- --updateSnapshot",
    "start": "yarn run webapp:dev --",
    "start-tls": "yarn run webapp:dev -- --env.tls",
    "pretest": "yarn run lint",
    "test": "yarn run jest --",
    "test-ci": "yarn run lint && yarn run jest:update --",
    "test:watch": "yarn run jest -- --watch",
    "webapp:build": "yarn run clean-www && yarn run webapp:build:dev --",
    "webapp:build:dev": "webpack --config webpack/webpack.dev.js --env stats=minimal",
    "webapp:build:prod": "webpack --config webpack/webpack.prod.js --progress=profile",
    "webapp:dev": "yarn run webpack-dev-server -- --config webpack/webpack.dev.js --env stats=minimal",
    "webapp:dev-verbose": "yarn run webpack-dev-server -- --config webpack/webpack.dev.js --progress=profile --env stats=normal",
    "webapp:prod": "yarn run clean-www && yarn run webapp:build:prod --",
    "webapp:test": "yarn run test --",
    "webpack-dev-server": "webpack serve",
    "prepare": "husky install",
    "docker:db:up": "docker-compose -f src/main/docker/mysql.yml up -d",
    "docker:db:down": "docker-compose -f src/main/docker/mysql.yml down -v --remove-orphans",
    "docker:keycloak:up": "docker-compose -f src/main/docker/keycloak.yml up -d",
    "docker:keycloak:down": "docker-compose -f src/main/docker/keycloak.yml down -v --remove-orphans",
    "docker:redis:up": "docker-compose -f src/main/docker/redis.yml up -d",
    "docker:redis:down": "docker-compose -f src/main/docker/redis.yml down -v --remove-orphans",
    "docker:app:up": "docker-compose -f src/main/docker/app.yml up -d oncokb-curation-app",
    "docker:others:await": "echo \"Waiting for keycloak to start\" && wait-on http-get://localhost:8080/auth/realms/oncokb-curation -t 30000 && echo \"keycloak started\" || echo \"keycloak not running, make sure oauth2 server is running\"",
    "predocker:others:up": "",
    "docker:others:up": "yarn run docker:keycloak:up && yarn run docker:redis:up",
    "docker:others:down": "yarn run docker:keycloak:down && yarn run docker:redis:down",
    "ci:e2e:prepare:docker": "yarn run docker:db:up && yarn run docker:others:up && docker ps -a",
    "ci:e2e:prepare": "yarn run ci:e2e:prepare:docker",
    "ci:e2e:teardown:docker": "yarn run docker:db:down --if-present && yarn run docker:others:down && docker ps -a",
    "ci:e2e:teardown": "yarn run ci:e2e:teardown:docker",
    "backend:info": "./mvnw -ntp enforcer:display-info --batch-mode",
    "backend:doc:test": "./mvnw -ntp javadoc:javadoc --batch-mode",
    "backend:nohttp:test": "./mvnw -ntp checkstyle:check --batch-mode",
    "backend:start": "./mvnw -P-webapp",
    "java:jar": "./mvnw -ntp verify -DskipTests --batch-mode",
    "java:war": "./mvnw -ntp verify -DskipTests --batch-mode -Pwar",
    "java:docker": "./mvnw -ntp verify -DskipTests jib:dockerBuild",
    "java:docker:arm64": "yarn run java:docker -- -Djib-maven-plugin.architecture=arm64",
    "backend:unit:test": "./mvnw -ntp -P-webapp verify --batch-mode -Dlogging.level.ROOT=OFF -Dlogging.level.org.zalando=OFF -Dlogging.level.tech.jhipster=OFF -Dlogging.level.org.mskcc.oncokb.curation=OFF -Dlogging.level.org.springframework=OFF -Dlogging.level.org.springframework.web=OFF -Dlogging.level.org.springframework.security=OFF",
    "backend:build-cache": "./mvnw dependency:go-offline",
    "backend:debug": "./mvnw -Dspring-boot.run.jvmArguments=\"-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=*:8000\"",
    "java:jar:dev": "yarn run java:jar -- -Pdev,webapp",
    "java:jar:prod": "yarn run java:jar -- -Pprod",
    "java:war:dev": "yarn run java:war -- -Pdev,webapp",
    "java:war:prod": "yarn run java:war -- -Pprod",
    "java:docker:dev": "yarn run java:docker -- -Pdev,webapp",
    "java:docker:prod": "yarn run java:docker -- -Pprod",
    "ci:backend:test": "yarn run backend:info && yarn run backend:doc:test && yarn run backend:nohttp:test && yarn run backend:unit:test",
    "ci:server:package": "yarn run java:$yarn_package_config_packaging:$yarn_package_config_default_environment",
    "ci:e2e:package": "yarn run java:$yarn_package_config_packaging:$yarn_package_config_default_environment -- -Pe2e -Denforcer.skip=true",
    "preci:e2e:server:start": "yarn run docker:db:await --if-present && yarn run docker:others:await --if-present",
    "ci:e2e:server:start": "java -jar target/e2e.$yarn_package_config_packaging --spring.profiles.active=$yarn_package_config_default_environment -Dlogging.level.ROOT=OFF -Dlogging.level.org.zalando=OFF -Dlogging.level.tech.jhipster=OFF -Dlogging.level.org.mskcc.oncokb.curation=OFF -Dlogging.level.org.springframework=OFF -Dlogging.level.org.springframework.web=OFF -Dlogging.level.org.springframework.security=OFF --logging.level.org.springframework.web=ERROR"
  },
  "config": {
    "backend_port": "9090",
    "default_environment": "prod",
    "packaging": "jar"
  },
