<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity GenomeFragment.
    -->
    <changeSet id="20220401174639-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="seq_region"
                                 baseTableName="genome_fragment"
                                 constraintName="fk_genome_fragment__seq_region_name"
                                 referencedColumnNames="name"
                                 referencedTableName="seq_region"/>

        <addForeignKeyConstraint baseColumnNames="transcript_id"
                                 baseTableName="genome_fragment"
                                 constraintName="fk_genome_fragment__transcript_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transcript"/>
    </changeSet>
</databaseChangeLog>
